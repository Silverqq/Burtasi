    <Button fx:id="AddEventSport" layoutX="217.0" layoutY="322.0" mnemonicParsing="false" onAction="#AddSport" prefHeight="36.0" prefWidth="169.0" style="-fx-background-color: gray;" text="Добавить Тренировку" textFill="WHITE">
