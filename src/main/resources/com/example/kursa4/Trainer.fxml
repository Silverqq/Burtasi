        <font>
            <Font name="System Bold Italic" size="25.0" />
        </font>
    </Label>
    <Button fx:id="Return" layoutX="15.0" layoutY="14.0" mnemonicParsing="false" onAction="#Return" prefHeight="36.0" prefWidth="105.0" style="-fx-background-color: gray;" text="Вернуться" textFill="WHITE">
        <font>
            <Font name="System Bold Italic" size="15.0" />
        </font>
    </Button>
