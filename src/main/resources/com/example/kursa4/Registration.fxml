<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="Reg" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.kursa4.Registration">
   <Label layoutX="232.0" layoutY="86.0" prefHeight="24.0" prefWidth="245.0" text="Регистрация" textAlignment="CENTER">
      <font>
         <Font name="Century Schoolbook" size="22.0" />
      </font>
   </Label>
   <TextField fx:id="Name" layoutX="139.0" layoutY="129.0" prefHeight="23.0" prefWidth="126.0" promptText="Имя" />
   <TextField fx:id="Login" layoutX="139.0" layoutY="175.0" prefHeight="23.0" prefWidth="126.0" promptText="Логин" />
   <TextField fx:id="LastName" layoutX="329.0" layoutY="129.0" prefHeight="23.0" prefWidth="126.0" promptText="Фамилия" />
   <Button fx:id="Registration" layoutX="225.0" layoutY="226.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="150.0" style="-fx-background-color: black;" text="Зарегистрироваться" textFill="WHITE">
      <font>
         <Font name="Times New Roman" size="14.0" />
      </font>
   </Button>
   <PasswordField fx:id="Password" layoutX="329.0" layoutY="175.0" prefHeight="25.0" prefWidth="126.0" promptText="Пароль" />
   <Button fx:id="Return" layoutX="36.0" layoutY="32.0" mnemonicParsing="false" onAction="#Return" text="Вернуться">
      <font>
         <Font name="Calibri Bold" size="13.0" />
      </font>
   </Button>
</AnchorPane>
